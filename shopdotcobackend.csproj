    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>ef5fb774-ebb5-43ca-ad26-02095ad05316</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
